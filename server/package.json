{
  "name": "notes-fly",
  "version": "1.0.0",
  "description": "Simple Notes Web App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hari-vickey/Notes-Fly.git"
  },
  "keywords": [
    "Notes",
    "MERN",
    "full-stack",
    "web"
  ],
  "author": "Hari Vikinesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hari-vickey/Notes-Fly/issues"
  },
  "homepage": "https://github.com/hari-vickey/Notes-Fly#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mongoose": "^7.0.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
